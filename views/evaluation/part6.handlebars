                <tr>
                  <th>Rate</th>
                  <th>Scale</th>
                </tr>
                <tr>
                  <td><input id="input-6" name="input-6" class="rating rating-loading" value="1" data-min="0" data-max="4" data-step="1" data-readonly="true" data-size="xs"></td>
